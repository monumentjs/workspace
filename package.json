{
  "name": "monument",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "migrations:create": "nx migrate latest",
    "migrations:run": "nx migrate --run-migrations",
    "docs": "typedoc --entryPointStrategy packages packages/*",
    "ci:build": "nx affected --target=build --base=last-release",
    "ci:test": "nx affected --target=test --base=last-release",
    "ci:lint": "nx affected --target=lint --base=last-release",
    "ci:release": "nx affected --target=release --base=last-release --parallel=1",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@jscutlery/semver": "^2.27.1",
    "@nrwl/cli": "15.8.1",
    "@nrwl/devkit": "15.8.1",
    "@nrwl/eslint-plugin-nx": "15.8.1",
    "@nrwl/jest": "15.8.1",
    "@nrwl/js": "15.8.1",
    "@nrwl/linter": "15.8.1",
    "@nrwl/nx-cloud": "15.1.1",
    "@nrwl/workspace": "15.8.1",
    "@types/jest": "29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "^13.0.3",
    "ngx-deploy-npm": "^4.2.2",
    "nx": "15.8.1",
    "prettier": "^2.6.2",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typedoc": "^0.23.16",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "packages/**/*.{ts}": "prettier --write"
  },
  "engines": {
    "node": ">=16"
  }
}

